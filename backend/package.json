{
  "name": "agentcare-backend",
  "version": "2.0.0",
  "description": "AgentCare Backend - Multi-Agent Healthcare Scheduling API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.js --quiet",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage .nyc_output",
    "db:migrate": "../scripts/db-setup.sh --no-demo-data",
    "db:seed": "../scripts/db-setup.sh",
    "health": "curl -s http://localhost:3000/health | jq .",
    "status": "curl -s http://localhost:3000/api/v1/agents/status | jq .",
    "metrics": "curl -s http://localhost:3000/api/v1/metrics | jq .",
    "security:deps": "npm audit --audit-level moderate"
  },
  "keywords": [
    "healthcare",
    "ai",
    "agents", 
    "scheduling",
    "llm",
    "ollama",
    "api",
    "nodejs",
    "typescript"
  ],
  "author": "Vishal Mishra",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "redis": "^4.6.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.2",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.19.4",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "ollama": ">=0.1.0"
  }
} 